@go.Package { name = "github.com/kdeps/schema/gen/env" }

module org.kdeps.pkl.ENV

import "package://pkg.pkl-lang.org/pkl-go/pkl.golang@0.5.0#/go.pkl"

class ResourceEnv {
  hidden idStringRegex = Regex(#"(^\w+$)"#)

  hidden isValidENV = (str) ->
  if (str.matches(idStringRegex))
    true
  else
    throw("Error: Invalid Environment Variable: The ENV VAR contains invalid characters. Please ensure it only includes alphanumeric characters (letters and numbers) and is not empty.")

  name: String(isValidENV)
  exec: String?
  value: String?
  input: String?
}
